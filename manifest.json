{
  "manifest_version": 3,
  "name": "CodeForces Rating Graph Hiding Extension",
  "version": "1.0",
  "description": "CodeForces Rating Graph Hiding Extension",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://codeforces.com/profile/*"
      ]
    }
  ]
}